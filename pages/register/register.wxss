.logo{
  margin: 42rpx auto 60rpx auto;
  width: 208rpx;
  height: 74rpx;
}
.logo image{
  width: 100%;
  height: 100rpx;
}
.Verification{
  width: 632rpx;
  height: 118rpx;
  border-bottom: 1rpx solid #ddd;
  display: flex;
  margin: 0 auto;
  align-items: center;
  box-sizing: border-box;
}
.Verification text:first-child{
  font-size: 26rpx;
  color: #333;
  margin-right:86rpx; 
}
.Verification input{
  font-size: 26rpx;
  color: #999;
}
.name text:first-child{
  margin-right: 114rpx;
}
.phone-number text:nth-child(3){
  font-size: 26rpx;
  color: #333;
}
.Agreement{
  width: 632rpx;
  margin: 20rpx auto;
  font-size: 26rpx;
  color: #999;
}
.Agreement text{
  color: #494c5d;
}
